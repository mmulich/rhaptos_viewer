{% extends "base.jinja2" %}
{% block head_title %}
  {{ module_title }} | {{ super() }}
{% endblock %}
{% block content %}
  <h1 id="collection-title">{{ collection_title }}</h1>
  <div class="row-fluid">
    <div id="collection-contents-tree" class="span4">
      {{ collection_contents_tree|safe }}
    </div>
    <div id="collection-body" class="span8">
      {{ collection_body|safe }}
    </div>
  </div>
{% endblock %}
